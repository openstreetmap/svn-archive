   </value>
   <value tag="pedestrian" >
    <description locale="en" >Pedestrian only road</description>
   </value>
   <value tag="footway" >
    <description locale="en" >Footway</description>
   </value>
   <value tag="cycleway" >
    <description locale="en" >Cycleway</description>
   </value>
   <value tag="bridleway" >

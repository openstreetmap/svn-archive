   </value>
   <value tag="unsurfaced" >
    <description locale="en" >Unsurfaced road (old tag)</description>
   </value>
  </widget>
  <widget tag="landuse" type="combo" >
   <link src="http://wiki.openstreetmap.org/index.php/Key:landuse" />

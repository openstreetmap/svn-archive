<maps>
    <map name="Computerteddy" boundary="Germany" comment="" updated="Weekly">
        <lang iso="de" name="Computerteddy"  boundary="Deutschland" comment="" updated="Woechentlich"/>
        <!-- routing: yes, no -->
        <!-- height display: no, shading or lines -->
        <!-- address search: no, pois or search -->
        <!-- typfile: no, optional or required -->
        <!-- colors(min): 2, 256 or 65000 -->
        <!-- nightmode: no, yes -->
        <attributes routing="no"/>
        <file name="gmapsupp.img" sizeMB="355"/>
        <download url="ftp://ftp5.gwdg.de/pub/misc/openstreetmap/teddynetz.de/latest/gmapsupp.img.gz" sizeMB="285"/>
        <picture url="Computerteddy.bmp"/>
        <details url="http://wiki.openstreetmap.org/wiki/User:Computerteddy"/>
        <!-- following is currently unused -->
        <boundary>
            <!-- bbox -->
            <!-- center position -->
            <continent>europe</continent>
            <country>germany</country>
        </boundary>
         <!-- yes/no or better: good, bad, usable, ...? -->
        <usage hiking="yes" climbing="no" cycling="yes" mtb="no" skiing="no" car="yes" motorcycle="yes" marine="no"/>
    </map>    


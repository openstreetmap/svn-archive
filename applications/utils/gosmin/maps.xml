        <details url="http://wiki.openstreetmap.org/wiki/All_in_one_Garmin_Map"/>
    </map>    

    <map name="All in One" boundary="Europe" comment="Lot's of OSM features" updated="Weekly">
        <lang iso="de" name="All in One"  boundary="Europa" comment="Moeglichst viele OSM Elemente" updated="Woechentlich"/>
        <attributes routing="yes"/>
        <file name="gmapsupp.img" sizeMB="1800"/>

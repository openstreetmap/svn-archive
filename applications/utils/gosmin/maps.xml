<maps>
    <map name="All in One" boundary="Germany" comment="Lot's of OSM features" updated="Weekly">
        <lang iso="de" name="All in One"  boundary="Deutschland" comment="Moeglichst viele OSM Elemente" updated="Woechentlich"/>
        <attributes routing="yes"/>
        <file name="gmapsupp.img" sizeMB="610"/>
        <download url="http://www.juergen-frank.de/osm/gmapsupps/germany/gmapsupp.img.bz2" sizeMB="460"/>

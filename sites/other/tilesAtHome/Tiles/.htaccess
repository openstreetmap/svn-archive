ErrorDocument 404 /~ojw/Tiles/404.png
RewriteEngine on
RewriteBase /~ojw/Tiles/

RewriteRule ^tile.php/([0-9]+)/([0-9]+)/([0-9]+).png$ 00$1/00000$2/00000$3.png
RewriteRule ^0*([0-9]{2})/0*([0-9]{3})([0-9]{3})/0*([0-9]{3})([0-9]{3}).png data/Tiles/$1/$2/$3/$4/$5.png

RewriteRule ^cycle.php/([0-9]+)/([0-9]+)/([0-9]+).png$ 00$1/00000$2/00000$3.png
RewriteRule ^0*([0-9]{2})/0*([0-9]{3})([0-9]{3})/0*([0-9]{3})([0-9]{3}).png data2/cycle/$1/$2/$3/$4/$5.png

RewriteRule ^maplint.php/([0-9]+)/([0-9]+)/([0-9]+).png$ 00$1/00000$2/00000$3.png
RewriteRule ^0*([0-9]{2})/0*([0-9]{3})([0-9]{3})/0*([0-9]{3})([0-9]{3}).png data2/maplint/$1/$2/$3/$4/$5.png

RewriteCond %{REQUEST_FILENAME} !-f 
RewriteRule .* db.php?tile=%{REQUEST_URI} 

RewriteRule ^(.*)_exists details.php
RewriteRule ^(.*)_details details.php

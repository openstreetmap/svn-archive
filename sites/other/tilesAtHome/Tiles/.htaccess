RewriteEngine on
RewriteBase /Tiles/

RewriteRule ^(\w+)(\.php)?/([0-9]+)/([0-9]+)/([0-9]+).png$ $1/00$3/00000$4/00000$5.png [C]
RewriteRule ^(\w+)/0*([0-9]{2})/0*([0-9]{3})([0-9]{3})/0*([0-9]{3})([0-9]{3}).png $1/$2/$3/$4/$5/$6.png

RewriteCond %{REQUEST_FILENAME} !-f 
RewriteRule .* db.php?tile=%{REQUEST_URI} 


<% content_for "optionals" do %>
  <div class="optionalbox">
    <span class="oboxheader">Tags</span>
    <br />
    <br />
    <% if @all_tags %>
      <% @all_tags.each do |tag| %>
        <%= link_to tag, :controller => 'trace', :action => @paging_action, :tag => tag %><br />
      <% end %>
    <% end %>
  </div>
<% end %>
